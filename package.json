{
  "name": "@oldsagesagar/qvium-quants",
  "private": true,
  "description": "NPM package for Jupyter Notebook",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neocosmicx-Quantum/qvium-quants.git"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "exports": {
    ".": "./build/index.js"
  },
  "scripts": {
    "playground": "node --import=./tsnode.esm.js --enable-source-maps bin/playground.ts",
    "build": "tsc",
    "clean": "del-cli build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prebuild": "nr clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check": "nr format && nr lint && nr typecheck"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@types/node": "^20.14.5",
    "del-cli": "^5.1.0",
    "eslint": "^9",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5"
  },
  "author": "NeoCosmicx",
  "license": "MIT",
  "keywords": [],
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "c8": {
    "exclude": [
      "tests/**"
    ]
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "peerDependencies": {
    "axios": "^1.7.9"
  }
}
